{
  "name": "react-image-responsive",
  "version": "1.0.12",
  "description": "An image that is responsive to its width on the screen (can be also a background image)",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Youpinadi/react-image-responsive"
  },
  "scripts": {
    "build": "babel --stage 0 src/index.js > index.js",
    "prepublish": "npm run build",
    "test": "BABEL_JEST_STAGE=0 jest"
  },
  "keywords": [
    "react-component",
    "image",
    "responsive"
  ],
  "author": "Nadir Kadem",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Youpinadi/react-image-responsive/issues"
  },
  "homepage": "https://github.com/Youpinadi/react-image-responsive",
  "dependencies": {
    "is-client": "0.0.2",
    "is-retina": "^1.0.3",
    "lodash.debounce": "^3.1.1",
    "object-assign": "^4.0.1",
    "babel": "^5.8.21"
  },
  "devDependencies": {
    "babel-jest": "^5.2.0",
    "jest-cli": "^0.4.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "react"
    ],
    "testFileExtensions": [
      "js"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "testDirectoryName": "test"
  }
}
